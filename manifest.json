{
  "manifest_version": 2,
  "name": "DestroyerX Auto-Fill Popup",
  "version": "1.0",
  "description": "Adds a floating popup for auto-filling forms.",
  "permissions": ["activeTab", "storage", "tabs"],
  "icons": {
    "128": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"],
      "css": ["styles/content.css"]
    }
  ]
}
