{
  "manifest_version": 2,
  "name": "Philo Tool",
  "version": "1.0",
  "description": "Automatically fills and submits form data with retry mechanism.",
  "permissions": ["activeTab", "storage", "tabs"],
  "icons": {
    "128": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"],
      "css": ["styles/content.css"]
    }
  ]
}
